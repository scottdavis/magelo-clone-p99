@import "compass/reset";
@import "compass/utilities";
@import "compass/css3";

$blue:#124a7e;
$gray:#3d3a3a;
$gold:#b77e2b;
body {
  p{
    color:black;
    padding:5px;
  }
  .google {
    text-align:center;
    padding-top:120px;
  }
  .pagination {
    background-color:#eee;
    text-align: center;
    padding: .3em; }
    .pagination a, .pagination span {
      padding: .2em .5em; }
    .pagination span.disabled {
      color: $gold; }
    .pagination span.current {
      font-weight: bold;
      color: black; }
    .pagination a {
      color: black;
      text-decoration: none; }
      .pagination a:hover, .pagination a:focus {
        border-color: $blue;
        background: white;
        color: $blue; }
    .pagination .page_info {
      color: #aaa;
      padding-top: .8em; }
    .pagination .prev_page, .pagination .next_page {
      border-width: 2px; }
    .pagination .prev_page {
      margin-right: 1em; }
    .pagination .next_page {
      margin-left: 1em; }
}

#wrapper {
  width:1024px;
  margin:{
    left:auto;
    right:auto;
  };
}

#header {
  #nav,.logo {
    @include float-left;
  }
  #nav p {
    @include float-left;
  }
}



#header, #wrapper {
  background-color:$blue;
}

a {
  color:white;
}
a:hover {
  color:#eee;
}
a:visited {
  color:#ccc;
}

.landing {
  .index {
    p{
      color:white;
      padding:5px;
    }
  }
}

h1 {
  color:white;
  padding:15px;
  font-size:30px;
  text-align:center;
}

h2 {
  color:white;
  padding:15px;
  font-size:25px;
  text-align:center; 
}


#nav {
  p{
    color:white;
    padding:5px;
    a {
      color:white;
    }
  }
}

.notice {
  color: green;
}

.alert {
  color: red;
}

div.field_with_errors {
  padding: 2px;
  background-color: red;
  display: table;
}

#errorExplanation {
  margin-left:auto;
  margin-right:auto;
  width: 450px;
  border: 2px solid red;
  padding: 7px;
  padding-bottom: 0;
  margin-bottom: 20px;
  background-color: #f0f0f0;
}

#errorExplanation h2 {
  font-size:19px;
  text-align: left;
  font-weight: bold;
  padding: 5px 5px 5px 15px;
  margin: -7px;
  margin-bottom: 0px;
  background-color: #c00;
  color: #fff;
}

#errorExplanation ul li {
  color:black;
  padding:5px;
  font-size: 12px;
  list-style: none;
  font-size:18px;
}

form {
  margin-left:auto;
  margin-right:auto;
  padding:10px;
  width:700px;
  @include border-radius(5px);
  color:black;
  h2{
    font-size: 24px;
    padding:5px;
    padding-bottom:10px;
  }
  label {
    color:black;
    font-size:20px;
  }
  p{
    padding:5px;
  }
  background-color:#eee;
}

.sessions, .registrations {
  h2 {
    font-size:24px;
    color:white;
    padding:10px;
  }
  .new {
    margin-top:50px;
    margin-left:50px;
    margin-right:50px;
    background-color:$blue;
    a {
      line-height:2;
      font-size:18px;
    }
  }
}



body {
  background-color:$gray;
}

.profiles {
  .actions {
    position:absolute;
    p {
      color:white;
    }
    top:400px;
    left:20px;
  }
  .right {
    @include float-right;
  }

  .left {
    @include float-left;
  }
  .slots {
    .item_data {
      width:350px;
    }
    .clickable_item_data {
      background-color:white;
      color:black;
      border:1px solid black;
    }
    .clickable_item_data:hover {
      background-color:#eee;
    }
  }
  .show {
    .item { 
      left:433px;
      padding:5px;
      position:absolute;
      top:336px;
      height:135px;
    }
    width:850px;
    height:600px;
    background-image:image_url('profile_template.png');
    .about {
      border:1px solid white;
      padding:4px;
      width:120px;
      @include border-radius(5px);
    }
    .char_stats {
      width:65px;
      .haste, .manaregeneration {
        padding-top:10px;
      }
    }
    #character {
      position :relative;
      #bag_area {
        height:240px;
        left:428px;
        position:absolute;
        top:50px;
        width:100px;
      }
      #bags {
        left:325px;
        position:absolute;
        top:235px;
        width:84px;
      }

      .character {
        width:253px;
        height:180px;
        color:white;
        .top, .middle, .bottom, .weapons {
          @include float-right;
          height:41px;
        }
      }
      .left_b {
        border-left:solid black 1px;
      }
      .bottom_b {
        border-bottom:solid black 1px;
      }
      .slot {
        @include float-left;
        width:40px;
        height:40px;
        border-top:solid black 1px;
        border-right:solid black 1px;
      }
      color:white;
      font-size:15px;
      padding-left:22px;
      padding-top:55px;
      width:385px;
      .highlighted {
        background-image:image_url('icon_hover.png');
      }
    }
  }
}


.item {
  font-size:15px;
  color:white;
  padding:5px;
  width:384px;
  .name {
    font-weight:bold;
    font-size:24;
  }
  .icon {
    @include float-right;
    padding-right:10px;
  }
}
.clear {
  clear:both;
}

.hidden {
  display:none;
}

#disqus_thread {
  width:700px;
  margin-left:auto;
  margin-right:auto;
  background-color:#eee;
  @include border-radius(5px);
  padding:10px;
  a{
    color:black;
  }
}


.container {
  p {
    a {
      color:black;
    }
  }
  ul {
    padding-left:20px;
    li {
      list-style-type: circle;
    }
  }
  margin:{
    left:auto;
    right:auto;
  };
  margin-bottom:10px;
  p {
    color:black;
  }
  padding:10px;
  width:700px;
  @include border-radius(5px);
  background-color:#eee;
}

.items {
  .show {
    .not_classic {
      @include float-right;
    }
    .item {
      background-color:$gray;
      @include border-radius(5px);
      pagging:10px;
    }
  }
  .search {
    #item {
      padding:10px;
      margin-right:30px;
      background-color:$gray;
      @include float-right;
      @include border-radius(5px);
    }
    .items {
      table {
        margin: {
         top:10px;
         bottom:10px; 
        }
        th {
          color:black;
          font-size:18px;
        }
        width:90%;
        margin-left:auto;
        margin-right:auto;
        td {
          padding:5px;
        }
        tr {
          a{
            color:black;
          }
        }
        tr.alt{
          a {
            color:$blue;
          }
          background-color:#DDD;
        }
      }
      margin-left:90px;
      @include float-left;
      width:500px;
      background-color:#eee;
      @include border-radius(5px);
    }
  }
}
.reports {
  table {
    width:100%;
    a {
      color:black;
    }
    tr.alt {
      background-color:#ddd;
    }
  }
